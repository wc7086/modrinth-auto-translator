{
  "name": "modrinth-auto-translator",
  "version": "1.0.0",
  "description": "Automated translation and build system for Modrinth App",
  "main": "scripts/extract-modrinth-translations.js",
  "scripts": {
    "extract": "node scripts/extract-modrinth-translations.js",
